<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxDialog" name="TeamSpeak_FormattingDialog">
		<title>TeamSpeak Formatting</title>
		<id_arg>0</id_arg>
		<object class="wxBoxSizer" variable="BoxSizer1" member="yes">
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="BoxSizer2" member="yes">
					<orient>wxVERTICAL</orient>
					<object class="sizeritem">
						<object class="wxBoxSizer" variable="BoxSizer3" member="yes">
							<object class="sizeritem">
								<object class="wxChoice" name="ID_MODECHOICE1" variable="ModeChoice" member="yes">
									<content>
										<item>Default</item>
									</content>
									<selection>0</selection>
									<size>83,21</size>
									<tooltip>Select a mode to format your text with.</tooltip>
									<handler function="OnChoice1Select" entry="EVT_CHOICE" />
								</object>
								<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_LETTERUSAGE1" variable="LetterUsage" member="yes">
									<label>length: 0</label>
									<size>106,16</size>
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
						</object>
						<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
					</object>
					<object class="sizeritem">
						<object class="wxTextCtrl" name="ID_INPUTTEXT1" variable="InputText" member="yes">
							<size>253,21</size>
							<focused>1</focused>
							<tooltip>Enter the text you want to format here. Upon entering the formatted text will be copied to clipboard.</tooltip>
							<handler function="OnTextCtrl1Text" entry="EVT_TEXT" />
						</object>
						<flag>wxALL|wxALIGN_BOTTOM|wxALIGN_CENTER_HORIZONTAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>4</border>
			</object>
		</object>
	</object>
</wxsmith>
